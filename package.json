    "vite": "^3.0.4",
    "vite-plugin-eslint": "^1.7.0",
    "vitest": "^0.21.1"
  },
  "eslintConfig": {
    "extends": "alloy",
    "rules": {
      "max-params": [
        "off",
        true
      ],
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  }
}
