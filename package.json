{
  "name": "cribl-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "eslint-config-alloy": "^4.6.2",
    "express": "^4.18.1",
